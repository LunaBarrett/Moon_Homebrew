        {
            "name": "The Gift's Glint",
            "source": "M:S_Items",
            "rarity": "uncommon",
            "entries": [
                "You can use an attack to cause a beam of radiant light to shoot foward from the Gift to hit a creature. That creature must succeed on a DC 17 Constitution saving throw or becoming blinded until the start of the creatures next turn"
            ],
            "reqAttune": false,
